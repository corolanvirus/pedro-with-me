{
  "name": "pedro-with-me",
  "displayName": "Pedro le Chat",
  "description": "Un petit chat qui vous accompagne pendant que vous codez",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pedro-with-me.showCat",
        "title": "Afficher Pedro le Chat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.87.0",
    "@types/node": "18.x",
    "typescript": "^5.3.3"
  }
}
